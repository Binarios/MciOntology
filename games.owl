:hasBorder rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain :LabyrinthBlock ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:oneOf [ rdf:type rdf:List ;
                                    rdf:first "D" ;
                                    rdf:rest [ rdf:type rdf:List ;
                                               rdf:first "L" ;
                                               rdf:rest [ rdf:type rdf:List ;
                                                          rdf:first "NONE" ;
                                                          rdf:rest [ rdf:type rdf:List ;
                                                                     rdf:first "R" ;
                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                rdf:first "U" ;
                                                                                rdf:rest rdf:nil
                                                                              ]
                                                                   ]
                                                        ]
                                             ]
                                  ]
                      ] .


