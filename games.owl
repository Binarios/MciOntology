                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "RD" ;
                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                             rdf:first "RU" ;
                                                                             rdf:rest rdf:nil
                                                                           ]
