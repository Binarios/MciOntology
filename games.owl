###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasAnswer
:hasAnswer rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasChoice ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Questions ;
           rdfs:range :Word .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasAntonym
:hasAntonym rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdf:type owl:SymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Word ;
            rdfs:range :Word .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasAssociatedImage
:hasAssociatedImage rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    owl:inverseOf :hasAssociatedSound ;
                    rdfs:domain :Sound ;
                    rdfs:range :Image .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasAssociatedSound
:hasAssociatedSound rdf:type owl:ObjectProperty ;
