                                                                 :changeNote "init" ;
                                                                 :createdBy "Ioannis IGOUMENAKIS" ;
                                                                 :createdDate "24-02-2019" ;
                                                                 :updateDate "24-02-2019" ;
                                                                 :updatedBy "Ioannis IGOUMENAKIS" ;
                                                                 owl:versionInfo "0.0.1" .
#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#connectsWith
:connectsWith rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :connectsWith ;
              rdf:type owl:SymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain :Piece ;
              rdfs:range :Piece .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasAntonym
:hasAntonym rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasWord ;
            rdfs:domain :AntonymsGame ;
            rdfs:range :Word ;
            owl:propertyDisjointWith :hasSynonim .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasBlock
:hasBlock rdf:type owl:ObjectProperty ;
          rdfs:domain :BlocksState ;
          rdfs:range :Block .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasBlockState
:hasBlockState rdf:type owl:ObjectProperty ;
               rdfs:domain :BlockMovementGame ;
               rdfs:range :BlocksState .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasDifference
:hasDifference rdf:type owl:ObjectProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain :Number ;
               rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasEndingImage
:hasEndingImage rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :ChronologicalOrder ;
                rdfs:range :MediumImage .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasHiddenNumber
:hasHiddenNumber rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasNumber ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :Calculation ;
                 rdfs:range :Number ;
                 owl:propertyDisjointWith :hasShownNumber .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasImage
:hasImage rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :ImageGames
                                      :SoundGames
                                    )
                      ] ;
          rdfs:range :Image .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasMainWord
:hasMainWord rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasWord ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :WordGames ;
             rdfs:range :Word .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasMathOperation
:hasMathOperation rdf:type owl:ObjectProperty ;
                  rdfs:domain :Calculation ;
                  rdfs:range :MathOperator .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasMovement
:hasMovement rdf:type owl:ObjectProperty ;
             rdfs:domain :BlockMovementGame ;
             rdfs:range :Movement .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasNextState
:hasNextState rdf:type owl:ObjectProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain :BlocksState .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasNumber
:hasNumber rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Calculation
                                       :Recall
                                     )
                       ] ;
           rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasPiece
:hasPiece rdf:type owl:ObjectProperty ;
          rdfs:domain :PieceGames ;
          rdfs:range :Piece .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasQuotient
:hasQuotient rdf:type owl:ObjectProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain :Number ;
             rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasRelatedImage
:hasRelatedImage rdf:type owl:ObjectProperty ,
                          owl:SymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :Image ;
                 rdfs:range :Image .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasShownNumber
:hasShownNumber rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasNumber ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Calculation
                                            :Recall
                                          )
                            ] ;
                rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasSimilarNumber
:hasSimilarNumber rdf:type owl:ObjectProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain :Number ;
                  rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasSound
:hasSound rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :SoundGames ;
          rdfs:range :Sound .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasStartingImage
:hasStartingImage rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :ChronologicalOrder ;
                  rdfs:range :MediumImage .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasSum
:hasSum rdf:type owl:ObjectProperty ,
                 owl:IrreflexiveProperty ;
        rdfs:domain :Number ;
        rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasSynonim
:hasSynonim rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasWord ;
            rdfs:domain :SynonimsGame ;
            rdfs:range :Word .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasTotal
:hasTotal rdf:type owl:ObjectProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :Number ;
          rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasWord
:hasWord rdf:type owl:ObjectProperty ;
         rdfs:domain :WordGames ;
         rdfs:range :Word .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#isAssociatedWithImage
:isAssociatedWithImage rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain :Sound ;
                       rdfs:range :Image .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#isBefore
:isBefore rdf:type owl:ObjectProperty ,
                   owl:SymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :ChronologicalImage ;
          rdfs:range :ChronologicalImage .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#isEqualNumber
:isEqualNumber rdf:type owl:ObjectProperty ,
                        owl:SymmetricProperty ;
               rdfs:domain :Number ;
               rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#isRecallNumber
:isRecallNumber rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Recall ;
                rdfs:range :Number .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#isSameWith
:isSameWith rdf:type owl:ObjectProperty ;
            owl:inverseOf :isSameWith ;
            rdf:type owl:SymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :SmallImage ;
            rdfs:range :SmallImage .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#displayTime
:displayTime rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :Recall ;
             rdfs:range xsd:long .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasDifficulty
:hasDifficulty rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Game ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasFiveDigitValue
:hasFiveDigitValue rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :hasNumberValue ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :FiveDigit ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:totalDigits 5
                                                       ]
                                                     )
                              ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasFourDigitValue
:hasFourDigitValue rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :hasNumberValue ;
                   rdfs:domain :FourDigit ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:totalDigits 4
                                                       ]
                                                     )
                              ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasId
:hasId rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Game
                                   :GameObject
                                 )
                   ] ;
       rdfs:range xsd:string .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasLevel
:hasLevel rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Game ;
          rdfs:range xsd:integer .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasNumberValue
:hasNumberValue rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Number ;
                rdfs:range xsd:long .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasOneDigitValue
:hasOneDigitValue rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasNumberValue ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :OneDigit ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:onDatatype xsd:integer ;
                               owl:withRestrictions ( [ xsd:totalDigits 1
                                                      ]
                                                    )
                             ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasSixDigitValue
:hasSixDigitValue rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasNumberValue ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :SixDigit ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:onDatatype xsd:long ;
                               owl:withRestrictions ( [ xsd:totalDigits "6"^^xsd:long
                                                      ]
                                                    )
                             ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasSymbol
:hasSymbol rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :MathOperator ;
           rdfs:range xsd:string .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasThreeDigitValue
:hasThreeDigitValue rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :hasNumberValue ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :ThreeDigit ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:onDatatype xsd:integer ;
                                 owl:withRestrictions ( [ xsd:totalDigits 3
                                                        ]
                                                      )
                               ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#hasTwoDigitValue
:hasTwoDigitValue rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasNumberValue ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :TwoDigit ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:onDatatype xsd:integer ;
                               owl:withRestrictions ( [ xsd:totalDigits 2
                                                      ]
                                                    )
                             ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#imageDescription
:imageDescription rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Image ;
                  rdfs:range xsd:string .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#imageLocation
:imageLocation rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Image ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#imageObject
:imageObject rdf:type owl:DatatypeProperty ;
             rdfs:domain :Image ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#imageSize
:imageSize rdf:type owl:DatatypeProperty ;
           rdfs:domain :Image ;
           rdfs:range xsd:decimal .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#isCurrentLevel
:isCurrentLevel rdf:type owl:DatatypeProperty ;
                rdfs:domain :Game ;
                rdfs:range xsd:positiveInteger .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#isWord
:isWord rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Word ;
        rdfs:range xsd:string .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#maxCompletionTime
:maxCompletionTime rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Game ;
                   rdfs:range xsd:long .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#movementName
:movementName rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Movement ;
              rdfs:range xsd:string .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#soundLocation
:soundLocation rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Sound ;
               rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#Addition
:Addition rdf:type owl:Class ;
          rdfs:subClassOf :MathOperator ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasSymbol ;
                            owl:hasValue "+"
                          ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#AntonymsGame
:AntonymsGame rdf:type owl:Class ;
              rdfs:subClassOf :WordGames ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasAntonym ;
                                owl:someValuesFrom :Word
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasMainWord ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Word
                              ] ;
              owl:disjointWith :SynonimsGame .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#BigImage
:BigImage rdf:type owl:Class ;
          rdfs:subClassOf :Image ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :imageSize ;
                            owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                 owl:onDatatype xsd:decimal ;
                                                 owl:withRestrictions ( [ xsd:minInclusive 0.5
                                                                        ]
                                                                      )
                                               ]
                          ] ;
          owl:disjointWith :SmallImage .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#Block
:Block rdf:type owl:Class ;
       rdfs:subClassOf :LogicalOrderBlocks .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#BlockMovementEasyGame
:BlockMovementEasyGame rdf:type owl:Class ;
                       rdfs:subClassOf :BlockMovementGame ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasMovement ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass :Movement
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :maxCompletionTime ;
                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                              owl:onDatatype xsd:long ;
                                                              owl:withRestrictions ( [ xsd:maxInclusive "1800"^^xsd:long
                                                                                     ]
                                                                                   )
                                                            ]
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasDifficulty ;
                                         owl:hasValue "easy"
                                       ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#BlockMovementGame
:BlockMovementGame rdf:type owl:Class ;
                   rdfs:subClassOf :LogicalOrder ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasBlockState ;
                                     owl:qualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                     owl:onClass :BlocksState
                                   ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#BlocksState
:BlocksState rdf:type owl:Class ;
             rdfs:subClassOf :LogicalOrderBlocks ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasBlock ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Block
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasBlock ;
                               owl:maxQualifiedCardinality "9"^^xsd:nonNegativeInteger ;
                               owl:onClass :Block
                             ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#BorderLinePiece
:BorderLinePiece rdf:type owl:Class ;
                 rdfs:subClassOf :BorderPiece ,
                                 [ rdf:type owl:Class ;
                                   owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :connectsWith ;
                                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                          owl:onClass :BorderLinePiece
                                                                        ]
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :connectsWith ;
                                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                          owl:onClass :CornerPiece
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :connectsWith ;
                                                   owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :BorderLinePiece
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :connectsWith ;
                                                   owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :CornerPiece
                                                 ]
                                               )
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :connectsWith ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :InteriorPiece
                                 ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#BorderPiece
:BorderPiece rdf:type owl:Class ;
             rdfs:subClassOf :Piece ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :connectsWith ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :InteriorPiece
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :connectsWith ;
                               owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                               owl:onClass :BorderPiece
                             ] ;
             owl:disjointWith :InteriorPiece .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#Calculation
:Calculation rdf:type owl:Class ;
             rdfs:subClassOf :NumberGames ;
             owl:disjointWith :Recall .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#ChronologicalImage
:ChronologicalImage rdf:type owl:Class ;
                    rdfs:subClassOf :MediumImage ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :isBefore ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :ChronologicalImage
                                    ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#ChronologicalOrder
:ChronologicalOrder rdf:type owl:Class ;
                    rdfs:subClassOf :ImageGames ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasImage ;
                                      owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                      owl:onClass :ChronologicalImage
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasEndingImage ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :ChronologicalImage
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasStartingImage ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :ChronologicalImage
                                    ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#CircleObj
:CircleObj rdf:type owl:Class ;
           rdfs:subClassOf :SmallImage .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#CornerPiece
:CornerPiece rdf:type owl:Class ;
             rdfs:subClassOf :BorderPiece ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :connectsWith ;
                               owl:allValuesFrom :BorderPiece
                             ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#Division
:Division rdf:type owl:Class ;
          rdfs:subClassOf :MathOperator ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasSymbol ;
                            owl:hasValue "/"
                          ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#EasyAntonymsGame
:EasyAntonymsGame rdf:type owl:Class ;
                  rdfs:subClassOf :AntonymsGame ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasWord ;
                                    owl:maxQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Word
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :maxCompletionTime ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:long ;
                                                         owl:withRestrictions ( [ xsd:maxInclusive "1800"^^xsd:long
                                                                                ]
                                                                              )
                                                       ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasDifficulty ;
                                    owl:hasValue "easy"
                                  ] .


###  http://www.semanticweb.org/iigou/diplomatiki/ontologies/Games#EasyCalculation
:EasyCalculation rdf:type owl:Class ;
                 rdfs:subClassOf :Calculation ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasHiddenNumber ;
                                   owl:allValuesFrom :TwoDigit
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasMathOperation ;
                                   owl:allValuesFrom [ rdf:type owl:Class ;
                                                       owl:unionOf ( :Addition
                                                                     :Equation
                                                                     :Substraction
                                                                   )
                                                     ]
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasShownNumber ;
